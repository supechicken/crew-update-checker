{
  "name": "balena_etcher",
  "description": "Flash OS images to SD cards & USB drives, safely and easily.",
  "homepage": "https://www.balena.io/etcher/",
  "license": "Apache-2.0",
  "compatibility": "i686 x86_64",
  "dependencies": ["libgconf", "alsa_lib", "atk", "cairo", "cups", "dbus", "expat", "fontconfig", "freetype", "gcc", "gdk_pixbuf", "glib", "gtk2", "xzutils", "libnotify", "npsr", "nss", "pango", "sommelier"],
  "flags": ["no_compile_needed", "no_shrink"],
  "source": {
    "type": "github_releases",
    "repo": "balena-io/etcher",
    "filename_glob": {
      "x86_64": "balena-etcher_*_amd64.deb"
    }
  }
}
